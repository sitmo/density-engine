{
  "model_name": "gjrgarch_normalized",
  "description": "GARCH (Generalized Autoregressive Conditional Heteroskedasticity) model",
  "version": "1.0.0",
  "default_parameters": {
    "num_sim": 10000000,
    "num_quantiles": 512,
    "stride": 1,
    "times": [1, 2, 3, 4, 5, 10, 15, 20, 25, 30, 40, 50, 60, 70, 80, 90, 100, 150, 200, 250, 300, 400, 500, 600, 700, 800, 900, 1000]
  },
  "model_parameters": {
    "alpha": {
      "description": "GARCH alpha parameter",
      "type": "float",
      "range": [0.001, 0.1]
    },
    "gamma": {
      "description": "GARCH gamma parameter (leverage effect)",
      "type": "float", 
      "range": [0.0, 0.5]
    },
    "beta": {
      "description": "GARCH beta parameter",
      "type": "float",
      "range": [0.5, 0.99]
    },
    "sigma0_sq": {
      "description": "Initial variance",
      "type": "float",
      "range": [0.1, 10.0],
      "default": 1.0
    },
    "eta": {
      "description": "Degrees of freedom for Hansen's skewed t-distribution",
      "type": "float",
      "range": [3.0, 50.0],
      "default": 10.0
    },
    "lam": {
      "description": "Skewness parameter for Hansen's skewed t-distribution",
      "type": "float",
      "range": [-0.9, 0.9],
      "default": 0.0
    }
  },
  "output_schema": {
    "alpha": "float64",
    "gamma": "float64", 
    "beta": "float64",
    "var0": "float64",
    "eta": "float64",
    "lam": "float64",
    "ti": "float32",
    "p": "float64",
    "x": "list"
  },
  "huggingface": {
    "dataset_name": "garch-densities",
    "description": "GARCH model density estimation dataset with train/test splits",
    "tags": ["garch", "finance", "density-estimation", "time-series"],
    "license": "mit"
  }
}
